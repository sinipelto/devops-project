{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",
  "RabbitMqManagement": {
    "BaseAddress": "http://rabbitmq:15672",
    "Username": "guest",
    "Password": "guest"
  },

  "Seq": {
    "ServerUrl": "http://seq",
    "ApiKey": "ApiGateway1234567890",
    "MinimumLevel": "Trace",
    "LevelOverride": {
      "Microsoft": "Warning"
    }
  },

  "HttpServ": {
    "Uri": "http://httpserv:80"
  },

  "DockerApi": {
    "MainContainerName": "apigateway",
    "MainContainerStopTimeout": 15, 
    "SocketPath": "unix:///app/docker.sock"
  }
}
